{
  "name": "@sense/webpack-ts-props-loader",
  "version": "0.1.0",
  "description": "Makes TypeScript types for React props available at runtime",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "dependencies": {
    "loader-utils": "^1.1.0",
    "lodash.memoize": "^4.1.2",
    "ts-simple-ast": "^10.1.0"
  },
  "peerDependencies": {
    "typescript": "^2.8.1"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/loader-utils": "^1.1.3",
    "@types/lodash.memoize": "^4.1.3",
    "jest": "^22.4.3",
    "memory-fs": "^0.4.1",
    "raw-loader": "^0.5.1",
    "ts-jest": "^22.4.2",
    "webpack": "^4.5.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.test.@(ts|tsx)"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ],
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
